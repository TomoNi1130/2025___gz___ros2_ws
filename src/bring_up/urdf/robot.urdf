<robot name="robot">
    <link name="robot_base">
        <visual>
            <geometry>
                <cylinder radius="0.4" length="0.1" />
            </geometry>
            <origin xyz="0 0 0.0" rpy="0 0 0" />
            <material name="gray">
                <color rgba="0.2 0.2 0.2 1.0" />
            </material>
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.4" length="0.1" />
            </geometry>
            <origin xyz="0 0 0.0" rpy="0 0 0" />
            <surface>
                <friction>
                    <ode>
                        <mu>0.0</mu>  <!-- 摩擦係数 -->
                        <mu2>1.0</mu2>  <!-- 横方向の摩擦係数 -->
                    </ode>
                </friction>
            </surface>
        </collision>
        <inertial>
            <mass value="26.0" />
            <inertia ixx="1.666666667" ixy="0.0" ixz="0.0" iyy="1.666666667" iyz="0.0" izz="1.666666667" />
        </inertial>
    </link>
    <link name="FL_wheel">
        <visual>
            <geometry>
                <cylinder radius="0.10" length="0.1" />
            </geometry>
            <material name="black">
                <color rgba="0.0 0.0 0.0 1.0" />
            </material>
        </visual>
        <collision>
            <geometry>
                <cylinder radius="0.10" length="0.1" />
            </geometry>
            <surface>
                <friction>
                    <ode>
                        <mu>1.0</mu>  <!-- 摩擦係数 -->
                        <mu2>0.1</mu2>  <!-- 横方向の摩擦係数 -->
                    </ode>
                </friction>
            </surface>
        </collision>
        <inertial>
            <mass value="1.0" />
            <inertia ixx="0.053333333" ixy="0.0" ixz="0.0" iyy="0.053333333" iyz="0.0" izz="0.04" />
        </inertial>
    </link>
    <joint name="FL_joint" type="revolute">
        <parent link="robot_base" />
        <child link="FL_wheel" />
        <axis xyz="0 0 1" />
        <origin xyz="0.282842712 0.282842712 0.0" rpy="1.571 0 2.356125" />
        <limit effort="2000" velocity="40.0" lower="-1000000000000" upper="1000000000000" />
    </joint>
    <gazebo>
        <plugin
            filename="gz-sim-joint-controller-system"
            name="gz::sim::systems::JointController">
            <joint_name>FL_joint</joint_name>
            <initial_velocity>0.0</initial_velocity>
            <topic>FL_v</topic>
        </plugin>
    </gazebo>
